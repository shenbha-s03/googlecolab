        "cat(\"b =\", b, \"\\n\")\n",
        "\n",
        "# Swapping logic\n",
        "t <- a\n",
        "a <- b\n",
        "b <- t\n",
        "\n",
        "cat(\"After swapping:\\n\")\n",
        "cat(\"a =\", a, \"\\n\")\n",
        "cat(\"b =\", b, \"\\n\")\n"
